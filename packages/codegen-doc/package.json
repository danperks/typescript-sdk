{
  "name": "@linear/codegen-doc",
  "private": true,
  "description": "Plugin for generating GraphQL fragments and documents from a schema",
  "version": "3.3.1",
  "author": "Linear Orbit, Inc",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/linear/linear",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external graphql --external @graphql-codegen/plugin-helpers --external @graphql-codegen/visitor-plugin-common"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "pascal-case": "^3.1.2",
    "tracer": "^1.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@graphql-codegen/plugin-helpers": "^2.7.0",
    "@graphql-codegen/visitor-plugin-common": "^2.13.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "auto-bind": "^4.0.0",
    "graphql": "^15.4.0",
    "rollup": "^2.38.3",
    "rollup-plugin-gzip": "^4.1.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
